{
  "command": "/usr/bin/python3 /usr/bin/s3vaultfuse.py --config-file=/etc/triliovault-object-store/triliovault-object-store-{{ backup_target_name }}.conf",
  "config_files": [
    {
      "dest": "/",
      "merge": true,
      "preserve_properties": true,
      "source": "/var/lib/kolla/config_files/triliovaultobjectstore/*"
    }
  ],
  "permissions": [
    {
      "owner": "nova:nova",
      "path": "/var/log/triliovault/",
      "recurse": true
    }
  ]
}